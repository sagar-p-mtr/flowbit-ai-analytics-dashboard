{
  "name": "flowbit-analytics",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "db:push": "cd apps/api && npm run db:push",
    "db:seed": "cd apps/api && npm run db:seed"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  }
}
